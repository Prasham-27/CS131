 cd ~/Documents/CS131
 mkdir ws56
 cd ws56
 sed -n '/^2\.0,/p' ../2019-01-h1.csv > vendor2.csv
 sed 's/[:,\"-]//g' ../2019-01-h1.csv > no-separators.csv\n
 sed 's/\.[0-9]*//g' no-separators.csv > no-fractions.csv\n
 \necho "Task 1.1: wc vendor2.csv result:" > ws56.txt
 \nwc vendor2.csv | head -3 >> ws56.txt
 \n\necho -e "\nTask 1.2: head -10 no-separators.csv result:" >> ws56.txt
 \nhead -10 no-separators.csv >> ws56.txt
 \n\necho -e "\nTask 1.3: head -10 no-fractions.csv result:" >> ws56.txt
 \nhead -10 no-fractions.csv >> ws56.txt
 \n\necho -e "\nTask 2.1: Frequency of tips for total amounts >= $10:" >> ws56.txt
 \nawk -F',' '$17 >= 10 {tips[$14]++} END {for (tip in tips) print tip, tips[tip]}' ../2019-01-h1.csv | head -20 >> ws56.txt
 \n\necho -e "\nTask 2.2: 20 most common tips:" >> ws56.txt
 \nawk -F',' '{tips[$14]++} END {for (tip in tips) print tip, tips[tip]}' ../2019-01-h1.csv | sort -k2nr | head -20 >> ws56.txt\n

